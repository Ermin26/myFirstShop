<% if(success.length){ %>
    <div class="alert alert-success alert-dismissible fade show w-lg-25 w-md-100 text-center" role="alert">
        <strong>
            <%= success %>
        </strong>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
            <span aria-hidden="true"></span>
        </button>
    </div>
    <% } %>


        <% if(error.length){ %>
            <div class="alert alert-danger alert-dismissible fade show w-lg-25 w-md-100 text-center" role="alert">
                <strong>
                    <%= error %>
                </strong>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true"></span>
                </button>
            </div>
            <% } %>